	email = rkopli@seegrid.com
	name = Ruban Kopli
[push]
	default = current
[submodule]
	recurse = true
	propogateBranches = true
	fetchJobs = 0
[core]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
[merge]
	conflictStyle = zdiff3
